	HIA.w r5,0	| r5 = aneg
	HIA.w r4,0	| r4 = apos
	HIA.w r3,0	| r3 = anul
	HIA.w r2,0	| r2 = aantal
	LEZ		| lees het n getal
	HIA r1,r0	| r1 = n

WHILE:	VGL r2,r1
	VSP GRG, print

	LEZ
	OPT.w r2,1

	VGL.w r0,0
	VSP NGEL,elseif
	OPT.w r3,1
	SPR while

ELSEIF:	VGL.w r0,0
	VSP KLG,else
	OPT.w r4,1
	SPR while

ELSE:	OPT.w r5,1
	SPR while

PRINT:	HIA r0,r3
	DRU
	HIA r0,r4
	DRU
	HIA r0,r5
	DRU

	STP
